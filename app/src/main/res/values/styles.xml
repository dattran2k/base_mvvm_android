    <style name="StyleDialogUpdateVersion" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:windowMinWidthMajor">66%</item>
        <item name="android:windowMinWidthMinor">66%</item>
    </style>


    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/color_app</item>
        <item name="colorPrimaryDark">@color/color_app</item>
        <item name="colorAccent">@color/color_app</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>

    <style name="AppTheme.Blue" parent="AppTheme">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#0281FD</item>
        <item name="colorPrimaryDark">#0281FD</item>
        <item name="colorAccent">#0281FD</item>
        <item name="android:windowDisablePreview">true</item>
    </style>

    <style name="ThemeDialog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>

    <style name="CustomBottomSheetDialogTheme" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/CustomBottomSheetStyle</item>
    </style>

    <style name="CustomBottomSheetStyle" parent="Widget.Design.BottomSheet.Modal">
